using System;
					
public class Program
{
	public static void Main()
	{
				int x;
            Console.WriteLine("Enter a number");
			x = Convert.ToInt32(Console.ReadLine());
            if (x % 2 == 0)
        {
            Console.WriteLine("Number is even");
        }
		else
		{
			Console.WriteLine("Number is Odd");
		}	
	}
}
